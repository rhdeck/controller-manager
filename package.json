{
  "name": "@raydeck/controller-manager",
  "version": "1.5.4",
  "description": "",
  "main": "lib/index.js",
  "bin": "lib/bin.js",
  "scripts": {
    "prepare": "install-peers",
    "pre-commit": "(git-branch-is --not main || (set -u && echo $ALLOWMAIN)) && yarn test",
    "test": "tsc --noEmit",
    "build": "ts-index-compiler && tsc && yarn pack -f package.tgz",
    "docs:bin": "echo \"No bin to document\"",
    "docs:lib": "typedoc --theme markdown --readme none --mode file --excludeNotExported --includeVersion --out docstemp  && rm README.md ; mv docstemp/README.md docstemp/_library.md",
    "docs:compile": "mkdir docstemp ; cp TOP.md docstemp/README.md ; concat-md docstemp > README.md && rm -rf docstemp",
    "docs": "rm -rf docstemp && yarn docs:lib && yarn docs:bin && yarn docs:compile",
    "prepublishOnly": "yarn build && yarn docs && git commit README.md -m \"docs: updating docs\" && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhdeck/base.git"
  },
  "author": "Ray Deck",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rhdeck/controller-manager/issues"
  },
  "homepage": "https://github.com/rhdeck/controller-manager#readme",
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@raydeck/livelink": "^1.5.2",
    "@types/node": "^14.0.27",
    "@types/prettier": "^2.1.0",
    "commander": "^6.0.0",
    "commanderdoc": "^0.1.11",
    "concat-md": "^0.3.5",
    "git-branch-is": "^4.0.0",
    "husky": "^4.2.5",
    "install-peers-cli": "^2.2.0",
    "prettier": "^2.1.1",
    "ts-index-compiler": "^1.0.0",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.0",
    "typedoc-plugin-markdown": "^2.4.0",
    "typescript": "^4.0.3"
  },
  "files": [
    "src",
    "lib",
    "serverless"
  ],
  "dependencies": {
    "@raydeck/id-components": "^1.0.3"
  },
  "peerDependencies": {
    "@raydeck/ddb-manager": "^3.1.0",
    "@raydeck/event-manager": "^1.1.0",
    "@raydeck/registry-manager": "^1.1.0",
    "@raydeck/session-manager": "^1.4.0",
    "aws-sdk": "^2.745.0"
  },
  "liveLink": {
    "ignoreMasks": [
      "package.tgz",
      "lib/**",
      "doctemp/**",
      "docstemp/**",
      "src/index.ts",
      "README.md",
      "src/make-index.ts"
    ]
  }
}
